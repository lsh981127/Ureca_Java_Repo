package com.ureca.school;

public class Employee {
	private String name;
	private int age;
	private char dept;
	
	
	
	
	
	public Employee(String name, int age, char dept) {
		super();
		this.name = name;
		this.age = age;
		this.dept = dept;
	}
	
	public Employee() {
		this("기본Em", 20, 'a');
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public char getDept() {
		return dept;
	}
	public void setDept(char dept) {
		this.dept = dept;
	}
	
	
	
	

}

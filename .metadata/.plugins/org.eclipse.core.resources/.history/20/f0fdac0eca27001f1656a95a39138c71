package com.ureca.school;

public class ManagerImpl {
	private Person[] pa;
	private int index;
	
	private ManagerImpl(int size) {
		pa = new Person[size];
		index = 0;
	}

	//2
	private ManagerImpl instance = new ManagerImpl();
	
	private ManagerImpl(int size) {
		pa = new Person[size];
	}
	
	// 1
	public ManagerImpl(Person[] pa, int index) {
		this(10);
	}
	
	
	
	public ManagerImpl getInstance() {
		return instance;
	}
	
	
}

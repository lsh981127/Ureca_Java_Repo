package com.ureca.school;
import java.util.*;
public class ListArray {

	public static void main(String[] args) {
		int[] ia = new int[3];
		ia[0] = 'a';
		System.out.println(ia);
		System.out.println();
		
		List<Integer>[] la = new List[3];    // 객체 선언부에는 new List<> X <> 주면 안된다
		System.out.println(Arrays.toString(la));
		la[0] = add(123)
		

	}

}

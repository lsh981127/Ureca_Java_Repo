package com.ureca.school;

public class ManagerImpl {
	private Person[] pa;
	private int index;

	//2
	private ManagerImpl instance = new ManagerImpl(10,0);
	
	private ManagerImpl(int size) {
		pa = new Person[size];
	}
	
	// 1
	public ManagerImpl(Person[] pa, int index) {
		this(10);
	}
	
	
	
	public static ManagerImpl getInstance() {   // 이떄 왜 static을 ㅆ는거지? 인스턴르로 사용할떄
		return instance;
	}
	
	
}

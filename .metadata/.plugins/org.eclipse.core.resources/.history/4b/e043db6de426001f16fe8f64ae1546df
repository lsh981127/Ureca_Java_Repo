package com.ureca.day1;

public class StringTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// '' : 이건 char 타입, 그리고 이건 1글자만 들어갈 수 있다!!
		// "" : string 타입
		
		String s1 = "홍길동";
		String s2 = "홍길동";
		System.out.println(s1==s2);
		System.out.println(s1.equals(s2));
		
	}

}
